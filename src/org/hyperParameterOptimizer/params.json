    "epsilon": {
      "type": "float",
      "low": 0.0,
      "high": 1.0
    },
    "parallelismFirst": {
      "type": "categorical",
      "choices": [
        true,
        false
      ]
    },
    "replaceIORs": {
      "type": "categorical",
      "choices": [
        true,
        false
      ]
    },
    "removeLoopActivities": {
      "type": "categorical",
      "choices": [
        true,
        false
      ]
    }
